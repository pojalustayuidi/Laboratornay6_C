#include <iostream>
using namespace std;
int main()
{
	setlocale(LC_ALL, "Russian");
	int a0[10], a1[10], a2[10],  a3[10],  a4[10],  a5[10], a6[10], a7[10], a8[10], a9[10];
	int* arr[10];
	arr[0] = a0;
	arr[1] = a1;
	arr[2] = a2;
	arr[3] = a3;
	arr[4] = a4;
	arr[5] = a5;
	arr[6] = a6;
	arr[7] = a7;
	arr[8] = a8;
	arr[9] = a9;
	for (int i = 0; i < 10; i++)
	{
		for (int j = 0; j < 10; j++)
		{
			arr[i][j] = rand() % 100;
		}
	}
	for (int i = 0; i < 10; i++)
	{
		for (int j = 0; j < 10; j++)
		{
			cout << arr[i][j] << ' ';
		}
		cout << endl;
	}

